xt: \"\"\n    };\n  },\n  methods: {\n    updateMessage() {\n      const message = {\n        id: this.messageId,\n        topicId: this.topicId,\n        title: this.title,\n        messageText: this.messageText\n      };\n      // call update in message service\n      messageService.updateTheMessage(this.messageId,message).then((response) => {\n        if (response.status === 200) {\n          this.$router.push({name: \"Messages\", params:{id: message.topicId}});\n        }\n      });\n    }\n  },\n  created() {\n    messageService\n      .get(this.messageId)\n      .then(response => {\n        this.$store.commit(\"SET_ACTIVE_MESSAGE\", response.data);\n        this.title = response.data.title;\n        this.messageText = response.data.messageText;\n      })\n      .catch(error => {\n        if (error.response.status == 404) {\n          this.$router.push(\"/not-found\");\n        }\n      });\n  }\n};\n</script>\n\n<style>\n</style>\n"]}]}